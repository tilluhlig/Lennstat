<?xml version='1.0' encoding='utf-8'?>
<widget id="tilluhlig.lennstat" version="1.0.8">
  <name>lennstat</name>
  <description>
    LennStat
  </description>
  <author>
    Till Uhlig
  </author>
  <preference name="EnableDeveloperConsole" value="$IS_DEBUG" />
  <preference name="UseStrictSSL" value="false" />
   

  <platform name="ios">
      <preference name="WKWebViewOnly" value="true" />
  
      <feature name="CDVWKWebViewEngine">
          <param name="ios-package" value="CDVWKWebViewEngine" />
      </feature>
  
      <preference name="CordovaWebViewEngine" value="CDVWKWebViewEngine" />
  
      <plugin name="cordova-plugin-wkwebview-engine" spec="~1.2.1" />
      <icon src="res/ios/Icon-App-83.5x83.5@2x.png" width="167" height="167" />
      <icon src="res/ios/Icon-App-76x76@2x.png" width="152" height="152" />
      <icon src="res/ios/Icon-App-76x76@1x.png" width="76" height="76" />
      <icon src="res/ios/Icon-App-60x60@3x.png" width="180" height="180" />
      <icon src="res/ios/Icon-App-60x60@2x.png" width="120" height="120" />
      <icon src="res/ios/Icon-App-40x40@2x.png" width="80" height="80" />
      <icon src="res/ios/Icon-App-40x40@1x.png" width="40" height="40" />
      <icon src="res/ios/Icon-App-29x29@3x.png" width="87" height="87" />
      <icon src="res/ios/Icon-App-29x29@2x.png" width="58" height="58" />
      <icon src="res/ios/Icon-App-29x29@1x.png" width="29" height="29" />
      <icon src="res/ios/Icon-App-20x20@3x.png" width="60" height="60" />
      <icon src="res/ios/Icon-App-20x20@1x.png" width="20" height="20" />
      <icon src="res/ios/playstore-icon.png" width="512" height="512" />
      <icon src="res/ios/playstore-icon@2x.png" width="1024" height="1024" />
      <splash src="res/screen/ios/default.png" width="640" height="960"/>
      <splash src="res/screen/ios/default2.png" width="768" height="1024"/>
  </platform>
</widget>