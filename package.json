    "chart.js": "1.0.1",
    "tabris": "^3.7.1",
    "tabris-decorators": "^3.7.1",
    "whatwg-fetch": "^3.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint-plugin-react": "^7.21.5",
    "typescript": "4.0.x"
  }
}
